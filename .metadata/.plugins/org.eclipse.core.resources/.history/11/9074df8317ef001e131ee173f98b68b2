package constructorTest;

class B {
	//필드
	int num;
	
	//메소드
	void printNum(int num) {
		System.out.println("num :" + num);
		System.out.println("필드 num : " + this.num);
		System.out.println("참조 값 : " + this);
	}
}





public class ClassTest2 {
	public static void main(String[] args) {
		B b1  = new B();
		b1.printNum(10);
		
		
	}
}
