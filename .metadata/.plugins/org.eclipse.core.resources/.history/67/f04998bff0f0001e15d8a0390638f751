package section05;

import java.util.Scanner;

public class EX05_06 {
	public static void main(String[] args) {
		//로직 구성
		//정수형 int 총합 sum 초기값
		//정수형 int i = 1 
		// do~ while 구문 이용 (조건식의 결과와 무관)
		
		int sum = 0;
		int i = 1;
		
		Scanner sc = new Scanner(System.in);
		
		do {
			sum += i;
			i++;
		} while (i <=10);
		
		System.out.println("합 : " + sum);
		
		
		
	}
}
