package section05;

import java.util.Scanner;

public class DoWhileTest {
	public static void main(String[] args) {
		//사용자가 0 이하의 숫자를 입력할 때까지 계속해서 입력을 받아보기
		
		Scanner sc = new Scanner(System.in);
		
		int i = 0;
		
		
		do {
			System.out.println("숫자를 입력하세요");
			i = sc.nextInt();
			
		}
		
	}
}
